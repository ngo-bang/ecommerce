<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="exampleModalLabel">Search Your Poducts</h5>
				<button type="button" class="btn-close" data-mdb-dismiss="modal" aria-label="Close"></button>
			</div>
			<div class="modal-body">
				<form action="/search" method="post">
					<div style="display:flex;justify-content: space-between;">
						<input style="width:100% ;margin-right: 5px;" type="text" name="key" placeholder="Search Here"
							required>
						<button type="submit" class="btn btn-primary">Search</button>
					</div>
				</form>
			</div>
			<div class="modal-footer">

			</div>
		</div>
	</div>
</div>

	<div id="wrapper " class="pt-5">

		<div class="container-fluid pt-5">

			<div class="card shadow mb-4 pt">
				<div class="card-header py-3">
					<h6 class="m-0 font-weight-bold text-primary"><span style="color:orange"> All Orders </span>
					</h6>
				</div>
				<div class="card-body">
					<div class="table-responsive">
						<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
							<thead>
								<tr>
									<th scope="col">Order ID & Date</th>

									<th scope="col">User</th>

									<th scope="col">Total</th>
									<th scope="col">Payment</th>

									<th scope="col">Details</th>


								</tr>
							</thead>
							<tbody>
								{{#each orders}}
								<tr>

									<td scope="row">
										<div><span style="font-weight: bold;">Time :</span>
											{{this.date}} </div>
										<br>
										<div><span style="font-weight: bold;">Time :</span>Date :{{this.date}}</div>
										<br>
										<div><span style="font-weight: bold;">Order ID :</span>{{this._id}}</div>
									</td>

									<td>

										<div><span style="font-weight: bold;">Address
												:</span>{{this.deliveryDetails.house}} </div>
										<br>
										<div><span style="font-weight: bold;">PIN
												:</span>{{this.deliveryDetails.pincode}} </div>
										<br>
										<div><span style="font-weight: bold;">Mobile
												:</span>{{this.deliveryDetails.mobile}}</div>
									</td>
									<td>
										<div><span style="font-weight: bold;">total Amount
												Paid:$</span>{{this.totalAmountPaid}}</div>
										<br>
										<div><span style="font-weight: bold;">total Amount Paid Refunded :
												$</span>{{this.amountToBeRefunded}}</div>
									</td>

									<td>
										<div><span style="font-weight: bold;">Method :</span>{{this.paymentMethod}}
										</div>
										<br>
										<div><span style="font-weight: bold;">Payment Status: </span>{{this.status}}
										</div>
									</td>

                                  
									<td><a href="/viewOrderProducts/{{this._id}}" class="btn primary-btn">Details</a>
									</td>
									

									



								</tr>
								{{/each}}

							</tbody>


						</table>
					</div>
				</div>
			</div>

		</div>

	</div>
	</div>
